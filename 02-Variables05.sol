pragma solidity ^0.8.7;

//Constant variables

contract HelloWeb3 {
    uint256 public constant trYear = 1923;
}

/*
Constant variables are variables whose value cannot be changed once they are defined. 
Constant variables can be of any data type. 
Constant variables are used to store data that is 
known at compile-time and does not need to be changed during the execution of the smart contract.
*/
